{
  "name": "imagina/flatly-theme",
  "type": "asgard-theme",
  "description": "An AsgardCMS Flatly bootswatch theme",
  "keywords": [
    "asgard-cms"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "info@asgardcms.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "composer/installers": "~1.0"
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan stylist:publish"
    ],
    "post-update-cmd": [
      "php artisan stylist:publish"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev",
      "dev-v10.x": "10.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
